    "as",
    "insert",
    "js",
    "javascript",
    "md"
  ],
  "author": "Spencer Scorcelletti <asimpletune@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asimpletune/readout/issues"
  },
  "homepage": "https://github.com/asimpletune/readout#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0"
  }
}
